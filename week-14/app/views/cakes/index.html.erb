<h1>Cake Orders</h1>

<table class="table">
  <thead>
    <tr>
      <th>Pickup At</th>
      <th>Assigned To</th>
      <th>Size</th>
      <th>Message</th>
      <th>Notes</th>
    </tr>
  </thead>
  <% @cakes.each do |cake| %>
    <tr>
      <td><%= cake.pickup %></td>
      <td><%= assignee(cake) %></td>
      <td><%= cake.size %></td>
      <td><%= cake.message %></td>
      <td><%= cake.notes %></td>
    </tr>
  <% end %>
</table>
